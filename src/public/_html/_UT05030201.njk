                    </div>
                </div>
                <script>
                    let startDate = new Date('2025-07-25');
                    new AirDatepicker('#date1', {
                        firstDay: 0,
                        startDate,
                        range: true,
                        navTitles: {days: '<i>yyyy</i>.MMMM'},
                        selectedDates: [startDate],
                        multipleDatesSeparator: '-',
                        position({$datepicker, $target, $pointer}) {
                            let coords = $target.getBoundingClientRect()
                            let top = window.scrollY + coords.height + coords.y + 10;
                            $datepicker.style.top = `${top}px`;
                            $datepicker.style.left = `${coords.x -2}px`;
                            $pointer.style.display = 'none';
                        }
                    });
                </script>
                <!-- //[2025.07.31 데이트피커 적용] -->
